{
  "name": "meter-solidly",
  "version": "1.0.0",
  "repository": "https://github.com/trungtin/meter-ve-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "create": "graph create meter/meter-solidly --node https://api.thegraph.com/deploy/",
    "create-local": "graph create meter/meter-solidly --node http://127.0.0.1:8020",
    "remove-local": "graph remove meter/meter-solidly --node http://127.0.0.1:8020",
    "build": "graph build",
    "deploy": "graph deploy meter/meter-solidly --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy meter/meter-solidly --debug --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
    "watch-local": "graph deploy graphprotocol/Uniswap2 --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
